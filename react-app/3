function ListGroup(){
  const items = [
    'New York',
    'San Francisco',
    'Tokyo',
    'Test',
    'Something'
  ];


  return (
    <>
      <h1>List</h1>
      <ul className="list-group">
        {items.map(item => <li className="list-group-item" key={items.indexOf(item)}>{item}</li>)}
      </ul>
    </>
  );
}

export default ListGroup;
